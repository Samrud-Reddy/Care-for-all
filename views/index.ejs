<body>
    <section class="carasoul-section">
        <img src="files/left.png" alt="<-" class="arrow left" onclick="go_left()">
        <div class="carasel">
            <div class="images">
                <% for( let i = 0; i < images; i++ ) { %>
                    <img src="files/img carasoul/<%= i+1 %>.jpeg" alt="NGO image" class="slideshow">
                <% } %>
                <img src="files/img carasoul/1.jpeg" alt="NGO image" class="slideshow">

